{
  "name": "boilerplate-module",
  "version": "0.1.0-dev",
  "description": "Boilerplate for a JavaScript module (browser + node).",
  "keywords": [
    "boilerplate"
  ],
  "homepage": "https://github.com/user/repo",
  "bugs": "https://github.com/user/repo/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:user/repo.git"
  },
  "author": "My Name (http://github.com/user)",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/boilerplate-module.min.js",
  "scripts": {
    "build": "npm run lint:fix && npm run build:build",
    "build:build": "rollup -c",
    "ci": "npm run lint && npm run build:build && npm run test:test",
    "lint": "eslint . && prettier . --check",
    "lint:fix": "eslint . --fix && prettier . --write",
    "test": "npm run build && npm run test:tests",
    "test:tests": "jest test"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.36.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
